.contentin{
	font-weight:bold;
}

.addressform{
	background-color:#FFF;
		width:570px;
		margin:0 auto;
		padding:20px;
			-webkit-border-radius: 15px;
	-moz-border-radius: 15px;
	border-radius: 15px;
	display:none;

}
#LeftTable{

	font-size:14px;
	border-collapse:collapse;
	border-spacing:0;
}
#LeftTable td{
	padding:5px 0;
}
#LeftTable input[type=text],#LeftTable select{
	font-family:Microsoft Yahei,"微软雅黑";
	height:23px;
}
#LeftTable textarea{
	font-family:Microsoft Yahei,"微软雅黑";
		font-size:14px;

}
#LeftTable .need{
	color:#F00;
}
#postcode{
	width:60px;
}
/*useful from qtip.css!!!!!!!!!!!!要用小窗口弹出的话！！！*/
.qtip {
    display: none;
    font-size: 10.5px;
    left: -28000px;
    line-height: 12px;
    max-width: 580px;
    min-width: 50px;
    position: absolute;
    top: -28000px;
}
/*----------------------*/
/*==========以下部分是必须的===========验证JS的CSS*/
.Validform_checktip{
	margin-left:8px;
	line-height:20px;
	height:20px;
	overflow:hidden;
	color:#999;
	font-size:12px;
	width:185px;
	}
.Validform_right{
	color:#71b83d;
	padding-left:20px;
	background:url(../../reg/images/right.png) no-repeat left center;
	}
.Validform_wrong{
	color:#E96845;
	padding-left:20px;
	white-space:nowrap;
	background:url(../../reg/images/error.png) no-repeat left center;
	}
.Validform_loading{
	padding-left:20px;
	background:url(../../reg/images/onLoad.gif) no-repeat left center;
	}
.Validform_error{
	background-color:#FFF;
}
#Validform_msg .Validform_title{
	line-height:25px;
	height:25px;
	text-align:left;
	font-weight:bold;
	padding:0 8px;
	color:#fff;
	position:relative;
	background-color:#000;
}
#Validform_msg a.Validform_close:link,#Validform_msg a.Validform_close:visited{
	line-height:22px;
	
	right:8px;
	top:0px;
	color:#fff;
	text-decoration:none;
	}
#Validform_msg a.Validform_close:hover{
	color:#cc0;
	}
#Validform_msg .Validform_info{
	padding:8px;
	border:1px solid #000;
	border-top:none;
	text-align:left;
}
/*----------------------*/

